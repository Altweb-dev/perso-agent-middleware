{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "little-night-a328",
	"main": "src/index.ts",
	"compatibility_date": "2024-09-23",
	"compatibility_flags": ["nodejs_compat_v2"],
	"observability": {
		"enabled": true
	},
	/**
	 * Environment Variables - Variáveis não-sensíveis
	 * Para desenvolvimento local, configure suas URLs aqui
	 * Para produção, atualize com os valores corretos
	 */
	"vars": {
		"SUPABASE_URL": "https://sua-url-supabase.supabase.co",
		"N8N_WEBHOOK_URL": "https://sua-instancia-n8n.com"
	}
	/**
	 * Secrets - Para dados sensíveis (configure via CLI)
	 * Execute os comandos abaixo para configurar as chaves sensíveis:
	 * 
	 * wrangler secret put OPENAI_API_KEY
	 * wrangler secret put SUPABASE_ANON_KEY
	 * wrangler secret put N8N_API_KEY
	 */
	/**
	 * Static Assets
	 * https://developers.cloudflare.com/workers/static-assets/binding/
	 */
	// "assets": { "directory": "./public/", "binding": "ASSETS" }
	/**
	 * Service Bindings (communicate between multiple Workers)
	 * https://developers.cloudflare.com/workers/wrangler/configuration/#service-bindings
	 */
	// "services": [{ "binding": "MY_SERVICE", "service": "my-service" }]
}